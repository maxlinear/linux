menuconfig MXL_NP_NET_CORE
	depends on NETDEVICES
	bool "Maxlinear NP Network device support"
	help
	  It contains basic network driver sets
	  required to drive Maxlinear NP(LGM/PRX300)
	  to support basic network packet handling.

if MXL_NP_NET_CORE
source "drivers/net/datapath/cqm/Kconfig"
source "drivers/net/datapath/dpm/Kconfig"
source "drivers/net/datapath/xpcs/Kconfig"
source "drivers/net/datapath/gswip/Kconfig"
source "drivers/net/datapath/p34x/Kconfig"
source "drivers/net/datapath/umt/Kconfig"
source "drivers/net/datapath/toe/Kconfig"
source "drivers/net/datapath/vpn/Kconfig"
source "drivers/net/datapath/qos_tc/Kconfig"
source "drivers/net/datapath/qos_notify/Kconfig"
source "drivers/net/datapath/dp_oam/Kconfig"
source "drivers/net/datapath/bridge_filter/Kconfig"
source "drivers/net/datapath/dp_eth_reinsert/Kconfig"
source "drivers/net/datapath/mib_counters/Kconfig"
source "drivers/net/datapath/directpath/Kconfig"
endif # MXL_NP_NET_CORE
